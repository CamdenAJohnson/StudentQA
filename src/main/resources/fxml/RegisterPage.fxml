<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.Controllers.RegisterController">
   <children>
      <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="165.0" promptText="Password" />
      <PasswordField fx:id="confirmPasswordField" layoutX="226.0" layoutY="200.0" promptText="Confirm Password" />
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="132.0" promptText="Username" />
      <Button layoutX="251.0" layoutY="243.0" mnemonicParsing="false" onAction="#handleRegister" prefHeight="25.0" prefWidth="93.0" text="Register" />
      <Button layoutX="254.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleBackToLogin" prefHeight="25.0" prefWidth="93.0" text="Back to Login" />
   </children>
</AnchorPane>
